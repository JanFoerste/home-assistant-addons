{
  "name": "Paperless-ngx",
  "version": "1.8.0-3",
  "slug": "paperless",
  "url": "https://github.com/paperless-ngx/paperless-ngx",
  "description": "Paperless is an application that manages your personal documents. With the help of a document scanner, paperless transforms your wieldy physical document binders into a searchable archive and provides many utilities for finding and managing your documents.",
  "image": "janfoerste/paperless-ngx-addon-{arch}",
  "webui": "http://[HOST]:[PORT:8000]",
  "ingress": true,
  "ingress_port": 8000,
  "stage": "experimental",
  "arch": ["armv7"],
  "startup": "system",
  "boot": "auto",
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web interface"
  },
  "map": ["share:rw"],
  "options": {
    "url": null,
    "staticurl": null,
    "filename": {
      "format": "{created_year}/{correspondent}/{title}"
    },
    "ocr": {
      "language": "eng",
      "languages": null
    },
    "default_superuser": {
      "username": null,
      "email": null,
      "password": null
    }
  },
  "schema": {
    "url": "str",
    "staticurl": "str",
    "filename": {
      "format": "str"
    },
    "ocr": {
      "language": "str",
      "languages": "str?"
    },
    "default_superuser": {
      "username": "str",
      "email": "email",
      "password": "password"
    }
  }
}
